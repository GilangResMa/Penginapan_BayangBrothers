{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images|fonts|img)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://bayang-brothers.vercel.app",
        "ASSET_URL": "https://bayang-brothers.vercel.app",
        "APP_KEY": "base64:L07Mu3wVqRXZ3snCdK+OU8fUNST7RBmmCV0mszY60FU=",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "aws.connect.psdb.cloud",
        "DB_PORT": "3306",
        "DB_DATABASE": "pbb",
        "DB_USERNAME": "bayangbrothers",
        "DB_PASSWORD": "pbbmerahputih",
        "DB_SSLMODE": "require",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "VIEW_CACHE_PATH": "/tmp/views",
        "SESSION_FILE_PATH": "/tmp/sessions",
        "CACHE_PATH": "/tmp/cache",
        
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr",
        "FILESYSTEM_DISK": "local",
        "QUEUE_CONNECTION": "sync"
    }
}